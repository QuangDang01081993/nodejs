<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Danh sach san pham</title>
  </head>
  <body>
    <table>
      <tr>
        <th>Stt</th>
        <th>Ten</th>
        <th>Hinh</th>
        <th>Id</th>
      </tr>
      <% arrSanPham.forEach((e,i) => {%>
        <tr>
          <td><%=i+1%></td>
          <td><%=e.title%></td>
          <td><img src="images/background/<%=e.hinh%>" alt="" width="120px"></td>
          <td><%=e.idPhim%></td>
          <td>
            <a href="/sua/<%=i%>">Sua</a> <br>
            <a href="/xoa/<%=i%>">Xoa</a>
          </td>
        </tr>
      <%});%>
    </table>
  </body>
</html>
